{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/static/*",
      "/images/*",
      "/favicon.ico",
      "/asset-manifest.json",
      "/manifest.json",
      "/robots.txt"
    ]
  },
  "routes": [
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ]
}